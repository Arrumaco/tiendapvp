<div class="clearfix">
  <div class="left" data-hook="registro_deposito_form_left">
    <%= f.hidden_field :id, :class => 'fullwidth title'  %>
    <%=  f.hidden_field :order_id %>

    <%= f.field_container :deposito_numero do %>
      <%= f.label :deposito_numero, t(:deposito_numero) %> <span class="required">*</span><br />
      <%= f.text_field :deposito_numero, :class => 'fullwidth title'  %>
      <%= f.error_message_on :deposito_numero %>
    <% end %>
    <%= f.field_container :deposito_fecha do %>
      <%= f.label :deposito_fecha, t(:deposito_fecha) %> <span class="required">*</span><br />
      <%= f.text_field :deposito_fecha, :class => 'fullwidth title'  %>
      <%= f.error_message_on :deposito_fecha %>
    <% end %>
  </div>
</div>
