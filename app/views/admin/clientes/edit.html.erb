<div data-hook="admin_clientes_edit_form_header">
  <%= render 'shared/error_messages', :target => @cliente %>
  <h1><%= t(:editing_cliente) %></h1>
</div>

<div data-hook="admin_cliente_edit_form">
<%= form_for [:admin, @cliente] do |f|   %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div data-hook="admin_cliente_edit_form_button">
    <%= render :partial => 'admin/shared/edit_resource_links' %>
  </div>
<% end %>
</div>


