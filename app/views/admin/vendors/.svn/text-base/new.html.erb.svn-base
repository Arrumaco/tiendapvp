<div data-hook="admin_vendor_new_form_header">
  <%= render 'shared/error_messages', :target => @vendor %>
  <h1><%= t(:new_vendor) %></h1>
</div>

<div data-hook="admin_vendor_new_form">
  <%= form_for [:admin, @vendor] do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>

    <div data-hook="admin_vendor_new_form_buttons">
      <%= render :partial => 'admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>
