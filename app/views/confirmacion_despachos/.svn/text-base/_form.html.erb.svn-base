<div class="clearfix">
  <div class="left" data-hook="registro_despacho_form_left">
    <%= f.hidden_field :id, :class => 'fullwidth title'  %>
    <%=  f.hidden_field :order_id %>

    <%= f.field_container :cantidad_despachada do %>
      <%= f.label :cantidad_despachada, t(:cantidad_despachada) %> <span class="required">*</span><br />
      <%= f.text_field :cantidad_despachada, :class => 'fullwidth title'  %>
      <%= f.error_message_on :cantidad_despachada %>
    <% end %>
    <%= f.field_container :quantity do %>
      <%= f.label :quantity, t(:quantity) %> <span class="required">*</span><br />
      <%= f.text_field :quantity, :class => 'fullwidth title', :disabled => 'disabled'  %>
      <%= f.error_message_on :quantity %>
    <% end %>
  </div>
</div>
